        <SDLNetLibrary>$([System.IO.Path]::GetFullPath('$(SDLOutputDir)$(Platform)\$(Configuration)\SDL_net.lib'))</SDLNetLibrary>
      </PropertyGroup>
    </When>
    <When Condition="'$(SDLLibraryType)'=='SDL2'">
      <PropertyGroup>
        <SDLDir>$([System.IO.Path]::GetFullPath('$(SourceRoot)_SDL2\'))</SDLDir>
        <SDLLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL2.lib'))</SDLLibrary>
        <SDLImageLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL2_image.lib'))</SDLImageLibrary>
        <SDLMixerLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL2_mixer.lib'))</SDLMixerLibrary>
        <SDLTTFLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL2_ttf.lib'))</SDLTTFLibrary>
        <FreeTypeLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\freetype.lib'))</FreeTypeLibrary>
      </PropertyGroup>
    </When>
    <When Condition="'$(SDLLibraryType)'=='LegacySDL'">
      <!--SDL libraries at root of SDL Directory(Likely to prevent using libs for both Win32 and x64)-->
      <PropertyGroup>
        <SDLDir>$([System.IO.Path]::GetFullPath('$(SourceRoot)_SDL\'))</SDLDir>
        <SDLLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL2.lib'))</SDLLibrary>
        <SDLImageLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL_image.lib'))</SDLImageLibrary>
        <SDLMixerLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL_mixer.lib'))</SDLMixerLibrary>
        <SDLTTFLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL_ttf.lib'))</SDLTTFLibrary>
        <FreeTypeLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\freetype.lib'))</FreeTypeLibrary>
      </PropertyGroup>
    </When>
    <Otherwise>
      <!--SDL 1.X Option-->
      <PropertyGroup>
        <SDLLibraryType Condition="'$(SDLLibraryType)'!='SDL'">SDL</SDLLibraryType>
        <SDLDir>$([System.IO.Path]::GetFullPath('$(SourceRoot)_SDL\'))</SDLDir>
        <SDLLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL.lib'))</SDLLibrary>
        <SDLImageLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL_image.lib'))</SDLImageLibrary>
        <SDLMixerLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL_mixer.lib'))</SDLMixerLibrary>
        <SDLTTFLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\SDL_ttf.lib'))</SDLTTFLibrary>
        <FreeTypeLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\$(Configuration)\freetype.lib'))</FreeTypeLibrary>
      </PropertyGroup>
    </Otherwise>
  </Choose>
