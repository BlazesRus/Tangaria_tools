  <!--
  Converting Relative to Absolute Path based on https://stackoverflow.com/questions/176931/how-can-i-get-msbuild-to-evaluate-and-print-the-full-path-when-given-a-relative
  $([System.IO.Path]::GetFullPath('XXX'))
  -->
  <!--Assuming Project Files is SubFolder of Main Project Root-->
  <!--Support for multiple SDL compiles; Change SDLLibraryType value to switch usage of SDL-->
  <PropertyGroup Label="UserMacros">
    <ParentOfTools>$([System.IO.Path]::GetFullPath('$(MSBuildProjectDirectory)\..\..\'))</ParentOfTools>
    <RootDirectory>$([System.IO.Path]::GetFullPath('$(ParentOfTools)\TangeriaCoop\'))</RootDirectory>
    <SourceRoot>$([System.IO.Path]::GetFullPath('$(RootDirectory)\src\'))</SourceRoot>
    <WinIncludeRoot>$([System.IO.Path]::GetFullPath('$(SourceRoot)\win\'))</WinIncludeRoot>
    <LibDataDir>$([System.IO.Path]::GetFullPath('$(RootDirectory)\lib\'))</LibDataDir>
    <SDLOutputDir>$([System.IO.Path]::GetFullPath('$(ParentOfTools)SDL2Output\'))</SDLOutputDir>
    <NetcodeType>WinsockNetcode</NetcodeType>
    <!--Later add netcode option SDLNetCode, to use SDL_net based code for the netcode instead-->
    <SDLLibraryType>SDL2</SDLLibraryType>
    <UserProjectSettingsFile>$([System.IO.Path]::GetFullPath('$(MSBuildProjectDirectory)TangeriaSDLClient.vcxproj.user'))</UserProjectSettingsFile>
    <!--Option of Modern for later C or ANSI for C99/C89-->
    <CStandardType>ANSI</CStandardType>
    <SPRoot></SPRoot>
  </PropertyGroup>
  <!--Import macro overrides from user props-->
  <Import Project="$(UserProjectSettingsFile)" Condition="exists('$(UserProjectSettingsFile)')" />
  <PropertyGroup>
    <!--     <ZLibDir>$([System.IO.Path]::GetFullPath('$(ParentOfTools)\zlib-win64\'))</ZLibDir> -->
    <ZLibLibrary Condition="'$(ZLibLibrary)'=='' Or !Exists('$(ZLibLibrary)')">$([System.IO.Path]::GetFullPath('$(WinIncludeRoot)$(Platform)\$(Configuration)\zlib.lib'))</ZLibLibrary>
    <ZLibDLL>$([System.IO.Path]::GetFullPath('$(WinIncludeRoot)$(Platform)\zlib.dll'))</ZLibDLL>
  </PropertyGroup>
  <PropertyGroup>
    <!--     <LibPngDir>$([System.IO.Path]::GetFullPath('$(ParentOfTools)\libpng\'))</LibPngDir> -->
    <!--Define LibPngDir to use non-default LibPng directory-->
    <LibPngLibrary Condition="!Exists('$(LibPngLibrary)')">$([System.IO.Path]::GetFullPath('$(WinIncludeRoot)$(Platform)\$(Configuration)\libpng.lib'))</LibPngLibrary>
    <LibPngDLL>$([System.IO.Path]::GetFullPath('$(WinIncludeRoot)$(Platform)\libpng.dll'))</LibPngDLL>
  </PropertyGroup>
  <Choose>
    <!-- Using SDL 2.X files (including SDL_Net later to replace WinSocket code)-->
    <When Condition="'$(SDLLibraryType)'=='ExternalSDL2'">
      <PropertyGroup>
        <SDLDir>$([System.IO.Path]::GetFullPath('$(ParentOfTools)SDL\include\'))</SDLDir>
        <SDLLibrary>$([System.IO.Path]::GetFullPath('$(SDLOutputDir)$(Platform)\$(Configuration)\SDL2.lib'))</SDLLibrary>
        <SDLImageDir>$([System.IO.Path]::GetFullPath('$(ParentOfTools)SDL_image\'))</SDLImageDir>
        <SDLImageLibrary>$([System.IO.Path]::GetFullPath('$(SDLOutputDir)$(Platform)\$(Configuration)\SDL2_image.lib'))</SDLImageLibrary>
        <SDLMixerDir>$([System.IO.Path]::GetFullPath('$(ParentOfTools)SDL_mixer\include\'))</SDLMixerDir>
        <SDLMixerLibrary>$([System.IO.Path]::GetFullPath('$(SDLOutputDir)$(Platform)\$(Configuration)\SDL2_mixer.lib'))</SDLMixerLibrary>
        <SDLTTFDir>$([System.IO.Path]::GetFullPath('$(ParentOfTools)SDL_ttf\'))</SDLTTFDir>
        <SDLTTFLibrary>$([System.IO.Path]::GetFullPath('$(SDLOutputDir)$(Platform)\$(Configuration)\SDL2_ttf.lib'))</SDLTTFLibrary>
        <FreeTypeLibrary>$([System.IO.Path]::GetFullPath('$(SDLOutputDir)$(Platform)\$(Configuration)\freetype.lib'))</FreeTypeLibrary>
        <SDLNetLibrary>$([System.IO.Path]::GetFullPath('$(SDLOutputDir)$(Platform)\$(Configuration)\SDL_net.lib'))</SDLNetLibrary>
      </PropertyGroup>
    </When>
    <When Condition="'$(SDLLibraryType)'=='SDL2'">
      <PropertyGroup>
        <SDLDir>$([System.IO.Path]::GetFullPath('$(SourceRoot)_SDL2\'))</SDLDir>
        <SDLLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\SDL2.lib'))</SDLLibrary>
        <SDLImageLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\SDL2_image.lib'))</SDLImageLibrary>
        <SDLMixerLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\SDL2_mixer.lib'))</SDLMixerLibrary>
        <SDLTTFLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\SDL2_ttf.lib'))</SDLTTFLibrary>
        <FreeTypeLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\freetype.lib'))</FreeTypeLibrary>
      </PropertyGroup>
    </When>
    <When Condition="'$(SDLLibraryType)'=='LegacySDL2'">
      <!--SDL libraries at root of SDL Directory(Likely to prevent using libs for both Win32 and x64)-->
      <PropertyGroup>
        <SDLDir>$([System.IO.Path]::GetFullPath('$(SourceRoot)_SDL2\'))</SDLDir>
        <SDLLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)SDL2.lib'))</SDLLibrary>
        <SDLImageLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)SDL2_image.lib'))</SDLImageLibrary>
        <SDLMixerLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)SDL2_mixer.lib'))</SDLMixerLibrary>
        <SDLTTFLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)SDL2_ttf.lib'))</SDLTTFLibrary>
        <FreeTypeLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)freetype.lib'))</FreeTypeLibrary>
      </PropertyGroup>
    </When>
    <When Condition="'$(SDLLibraryType)'=='LegacySDL'">
      <!--SDL libraries at root of SDL Directory(Likely to prevent using libs for both Win32 and x64)-->
      <PropertyGroup>
        <SDLDir>$([System.IO.Path]::GetFullPath('$(SourceRoot)_SDL2\'))</SDLDir>
        <SDLLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)SDL2.lib'))</SDLLibrary>
        <SDLImageLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)SDL2_image.lib'))</SDLImageLibrary>
        <SDLMixerLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)SDL2_mixer.lib'))</SDLMixerLibrary>
        <SDLTTFLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)SDL2_ttf.lib'))</SDLTTFLibrary>
        <FreeTypeLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)freetype.lib'))</FreeTypeLibrary>
      </PropertyGroup>
    </When>
    <Otherwise>
      <!--SDL 1.X Option-->
      <PropertyGroup>
        <SDLLibraryType Condition="'$(SDLLibraryType)'!='SDL'">SDL</SDLLibraryType>
        <SDLDir>$([System.IO.Path]::GetFullPath('$(SourceRoot)_SDL\'))</SDLDir>
        <SDLLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\SDL.lib'))</SDLLibrary>
        <SDLImageLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\SDL_image.lib'))</SDLImageLibrary>
        <SDLMixerLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\SDL_mixer.lib'))</SDLMixerLibrary>
        <SDLTTFLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\SDL_ttf.lib'))</SDLTTFLibrary>
        <FreeTypeLibrary>$([System.IO.Path]::GetFullPath('$(SDLDir)$(Platform)\freetype.lib'))</FreeTypeLibrary>
      </PropertyGroup>
    </Otherwise>
  </Choose>
